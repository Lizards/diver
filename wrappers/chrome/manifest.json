{
    "manifest_version": 2,
    "name": "Diver - A Web Traffic Monitor Tool",
    "version": "0.0.2",

    "description": "",
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },

    "browser_action": {},

    "author": "Lawrence Lau",
    "background": {
        "page": "eventpage.html",
        "persistent": false
    },
    "content_security_policy": "script-src 'self' https://cdn.jsdelivr.net; object-src 'self'",
    "devtools_page": "devtools.html",
    "minimum_chrome_version" : "57",
    "permissions": [
        "activeTab",
        "storage",
        "downloads"
    ],
    "sandbox": {
        "pages": ["sandbox.html"]
    }
}
