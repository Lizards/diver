{
    "manifest_version": 2,
    "name": "Diver",
    "description": "Dive into the network traffic",
    "version": "0.0.1",
    "devtools_page": "devtools.html",
    "minimum_chrome_version" : "57",
    "permissions": [
        "activeTab",
        "storage",
        "downloads"
    ],
    "background": {
        "page": "eventpage.html",
        "persistent": false
    },
    "sandbox": {
        "pages": ["sandbox.html"]
    },
    "content_security_policy": "script-src 'self' https://cdn.jsdelivr.net; object-src 'self'"
}
